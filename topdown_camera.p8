-- move player
-- call this function in _update()
function move_plyr()

	-- instead of just moving,
	-- first determine if the tile
	-- the player is trying to move
	-- to is something they can
	-- actually go to

	-- left
	if btnp(‚¨ÖÔ∏è) then
		-- target 1 tile left of plyr
		tx = plyr.x - 1
		ty = plyr.y
		
		-- set direction
		plyr.dir = ‚¨ÖÔ∏è
		plyr.n = 96
		plyr.flip = true
	end -- /if btnp(‚¨ÖÔ∏è)
	
	-- right
	if btnp(‚û°Ô∏è) then
		-- target 1 tile right of plyr
		tx = plyr.x + 1
		ty = plyr.y
		
		-- set direction
		plyr.dir = ‚û°Ô∏è
		plyr.n = 96
		plyr.flip = false
	end -- /if btnp(‚û°Ô∏è)
	
	-- up
	if btnp(‚¨ÜÔ∏è) then
		-- target 1 tile above plyr
		tx = plyr.x
		ty = plyr.y - 1
		
		-- set direction
		plyr.dir = ‚¨ÜÔ∏è
		plyr.n = 80
		plyr.flip = false
	end -- /if btnp(‚¨ÜÔ∏è)
	
	-- down
	if btnp(‚¨áÔ∏è) then
		-- target 1 tile below plyr
		tx = plyr.x
		ty = plyr.y + 1
		
		-- set direction
		plyr.dir = ‚¨áÔ∏è
		plyr.n = 64
		plyr.flip = false
	end -- /if btnp(‚¨áÔ∏è)

	-- sprite number of target tile
	tn = mget(tx,ty)
	
	-- sprite flags
	wall = 0
 
	-- true/false is flag 0 on
	-- for that sprite
	is_wall = fget(tn,wall)
 
	-- move to target if no wall
	if is_wall == false then
		plyr.x=tx
		plyr.y=ty
	-- play bump sound if blocked
	else
		if btn(‚¨ÖÔ∏è) or btn(‚û°Ô∏è)
		or btn(‚¨ÜÔ∏è) or btn(‚¨áÔ∏è) then
			sfx(0)
		end -- /if btn
	end -- /if is_wall == false
	
end -- /function move_plyr()
