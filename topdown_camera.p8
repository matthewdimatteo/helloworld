-- swap camera mode
-- call this function in _update()
function swap_cam()

	-- press z to swap cam mode
	if btnp(üÖæÔ∏è) then
	
		if mode == "follow" then
			mode = "screen"
		else
			mode = "follow"
		end -- /if mode
	
	end -- /if btnp(üÖæÔ∏è)

end -- /function swap_cam()
