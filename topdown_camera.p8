-- make player
-- call this function in _init()
function make_plyr()
	plyr = {} -- table
	plyr.n = 64 -- sprite number
	
	-- x,y tile coordinates
	plyr.x = 7
	plyr.y = 6

	-- tiles wide, tall
	-- multiply by 8 to get px
	plyr.w = 1
	plyr.h = 1

	-- direction
	plyr.dir = ‚¨áÔ∏è -- down
	
	-- whether to flip sprite
	plyr.flip= false
end -- /function make_plyr()
