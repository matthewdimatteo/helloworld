-- make player
function make_plyr()
	plyr = {}

	-- x,y tile coordinates
	-- multiply by 8 to get pixels
	plyr.x = 7
	plyr.y = 6

	-- player sprite
	plyr.n = 64 -- sprite number

	-- tiles wide, tall
	-- multiply by 8 to get pixels
	plyr.w = 1
	plyr.h = 1

	-- direction
	plyr.dir = "down"
	plyr.flip= false
 
end -- end make_plyr()
-->8
-- move player
function move_plyr()

	-- left
	if btnp(‚¨ÖÔ∏è) then
		tx = plyr.x - 1
		ty = plyr.y
		plyr.dir = "left"
		plyr.n = 66
		plyr.flip = true
	end -- end if btnp(‚¨ÖÔ∏è)
	
	-- right
	if btnp(‚û°Ô∏è) then
		tx = plyr.x + 1
		ty = plyr.y
		plyr.dir = "right"
		plyr.n = 66
		plyr.flip = false
	end -- end if btnp(‚û°Ô∏è)
	
	-- up
	if btnp(‚¨ÜÔ∏è) then
		ty = plyr.y - 1
		tx = plyr.x
		plyr.dir = "up"
		plyr.n = 65
		plyr.flip = false
	end -- end if btnp(‚¨ÜÔ∏è)
	
	-- down
	if btnp(‚¨áÔ∏è) then
		ty = plyr.y + 1
		tx = plyr.x
		plyr.dir = "down"
		plyr.n = 64
		plyr.flip = false
	end -- end if btnp(‚¨áÔ∏è)
	
	-- sprite number of target tile
	tn = mget(tx,ty)
 
	-- true/false is flag 0 on
	-- for that sprite
	is_solid = fget(tn,solid)
 
	-- move to target if not solid
	if is_solid == false then
		plyr.x=tx
		plyr.y=ty
	end -- end if is_solid == false
	
	-- coordinates of object tile
	objx=tx
	objy=ty
 
end -- end move_plyr()
