-- toggle target box display
function toggle()
	if btnp(üÖæÔ∏è) then
		if show == true then
			show = false
			text="üÖæÔ∏è = show target box"
		elseif show == false then
			show = true
			text="üÖæÔ∏è = hide target box"
		end -- end if/else show t/f
	end -- end if btnp(üÖæÔ∏è)
end -- end function toggle()
