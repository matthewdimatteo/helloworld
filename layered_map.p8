pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- rider game academy
-- layered map template
-- by matthew dimatteo

function _draw()
	cls() -- refresh screen
	
	-- first two values 0,16 are
	-- the location of the part
	-- of the map you want to draw
	
	-- middle two values 0,0 are
	-- where on the screen to draw
	-- that part of the map
	
	-- last two values 16,16 are
	-- the width and height of
	-- the part of the map to draw
	map(0,16,0,0,16,16)--background
	
	-- you can draw on top of the
	-- previous layer with another
	-- call to map()
	
	-- by default, this draws the
	-- top-left part of the map
	-- across the entire screen
	map() -- foreground
	
	-- draw player sprite
	spr(20,3*8,14*8)
end
