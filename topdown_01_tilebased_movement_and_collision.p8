-- make player
function make_plyr()
	plyr = {}

	-- x,y tile coordinates
	-- multiply by 8 to get pixels
	plyr.x = 2
	plyr.y = 2

	-- player sprite
	plyr.n = 64 -- sprite number

	-- tiles wide, tall
	-- multiply by 8 to get pixels
	plyr.w = 1
	plyr.h = 1

	-- direction
	plyr.dir = "down"
	plyr.flip= false
 
end -- end make_plyr()
