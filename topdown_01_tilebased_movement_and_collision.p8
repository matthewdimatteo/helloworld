-- topdown adventure example
-- example 01
-- tile-based movement/collision
-- by matthew dimatteo

-- tab 0: game loop
-- tab 1: make player
-- tab 2: move player
-- tab 3: toggle target box

function _init()
	make_plyr() -- tab 1
	
	-- target tile coordinates
	-- (where the player is trying
	-- to move to)
	tx = plyr.x
	ty = plyr.y
	
	-- show target box
	show=true
	text="üÖæÔ∏è = hide target box"
	
	-- sprite flags
	wall = 0 
 
end -- end function _init()

function _update()
	move_plyr() -- tab 2
	toggle() -- tab 3
end

function _draw()
	cls()
	map()
	spr(plyr.n,plyr.x*8,plyr.y*8,plyr.w,plyr.h,plyr.flip)
	
	-- draw target box
	if show == true then
		rect(tx*8,ty*8,8+tx*8,8+ty*8,7)
	end -- end if show == true
	
	-- instructions
	print(text,24,121,7)
	
end -- end function _draw()
