-- move player
function move_plyr()

	-- left
	if btnp(‚¨ÖÔ∏è) then
		tx = plyr.x - 1
		ty = plyr.y
		plyr.dir = "left"
		plyr.n = 66
		plyr.flip = true
	end -- end if btnp(‚¨ÖÔ∏è)
	
	-- right
	if btnp(‚û°Ô∏è) then
		tx = plyr.x + 1
		ty = plyr.y
		plyr.dir = "right"
		plyr.n = 66
		plyr.flip = false
	end -- end if btnp(‚û°Ô∏è)
	
	-- up
	if btnp(‚¨ÜÔ∏è) then
		ty = plyr.y - 1
		tx = plyr.x
		plyr.dir = "up"
		plyr.n = 65
		plyr.flip = false
	end -- end if btnp(‚¨ÜÔ∏è)
	
	-- down
	if btnp(‚¨áÔ∏è) then
		ty = plyr.y + 1
		tx = plyr.x
		plyr.dir = "down"
		plyr.n = 64
		plyr.flip = false
	end -- end if btnp(‚¨áÔ∏è)
	
	-- sprite number of target tile
	tn = mget(tx,ty)
 
	-- true/false is flag 0 on
	-- for that sprite
	is_wall = fget(tn,wall)
 
	-- move to target if no wall
	if is_wall == false then
		plyr.x=tx
		plyr.y=ty
	end -- end if is_wall == false
 
end -- end move_plyr()
