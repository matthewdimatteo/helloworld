-- rider game academy
-- sidescroller template
-- with physics
-- *** multiple levels ***
-- by matthew dimatteo

-- tab 0: game loop
-- tab 1: make player
-- tab 2: move player
-- tab 3: map collision function
-- tab 4: camera function
-- tab 5: *** level warp ***
-- tab 6: respawn function

-- runs once at start
function _init()
	
	-- physics forces
	fric = 0.85 -- friction
	grav = 0.3 -- gravity

	-- variables for plyr
	make_plyr() -- tab 1

	-- starting position for camera
	camx = 0
	camy = 0

	-- *** level number ***
	level = 1
	
end -- /function _init()

-- runs 30 times per second
function _update()
	move_plyr() --  tab 2
	cam_follow() -- tab 4
	level_warp() -- *** tab 5 ***
	respawn() -- tab 6
end -- /function _update()

-- runs 30 times per second
function _draw()
	cls() -- refresh screen
	map() -- draw map

	-- draw player sprite
	spr(plyr.n,plyr.x,plyr.y,plyr.w/8,plyr.h/8,plyr.flip)

	-- print hud
	print("level: "..level,camx+12,camy+2,0)
	print("start: "..plyr.startx..","..plyr.starty,camx+12,camy+10,0)
	print("cam  : "..flr(camx)..","..flr(camy),camx+12,camy+18,0)
end -- /function _draw()
-->8
-- make player
-- call in _init()
function make_plyr()

	-- table
	plyr = {} 
	
	-- sprite number
	plyr.n = 1 

	-- game data
	plyr.lives = 3
	plyr.health = 100

	-- player starting coordinates
	plyr.startx = 6*8 -- 48 pixels
	plyr.starty = 12*8 -- 96 pixels

	-- plug in starting coords
	plyr.x = plyr.startx
	plyr.y = plyr.starty

	-- width and height (8px)
	plyr.w = 8
	plyr.h = 8
	
	-- base speed
	plyr.xspd=0.5 -- x speed
	plyr.yspd=3 -- y speed
	
	-- active speed
	plyr.dx=0 -- change in x
	plyr.dy=0 -- change in y
	
	-- player state
	plyr.dir = ‚û°Ô∏è -- direction
	plyr.flip =false

	-- number of jumps left
	plyr.basejumps = 2
	plyr.jumps = plyr.basejumps
	
end -- /function make_plyr()
-->8
-- move player function
-- call in _update()
function move_plyr()
	
	-- apply friction so the plyr
	-- eventually stops moving
	plyr.dx *= fric
	
	-- apply gravity so the plyr
	-- does not float endlessly
	plyr.dy += grav
	
	-- move left
	if btn(‚¨ÖÔ∏è) then
		-- subtract from change in x
		plyr.dx -= plyr.xspd
		
		-- track player's direction
		plyr.dir = ‚¨ÖÔ∏è
		
		-- flip the sprite
		plyr.flip = true
	end -- /if btn(‚¨ÖÔ∏è)
	
	-- move right
	if btn(‚û°Ô∏è) then
		-- add to change in x
		plyr.dx += plyr.xspd

		-- track player's direction
		plyr.dir = ‚û°Ô∏è
		
		-- un-flip sprite
		plyr.flip = false
	end -- /if btn(‚¨ÖÔ∏è)
	
	-- jump
	if (btnp(‚¨ÜÔ∏è) or btnp(‚ùé))
	and plyr.jumps > 0
	then
		plyr.dy = -plyr.yspd
		plyr.landed=false
		plyr.jumps -= 1
	end -- /if btnp(‚¨ÜÔ∏è/‚ùé)
	
	-- test collision below
	if plyr.dy > 0 then
	
		-- stop falling when a solid
		-- tile is below the player
		if mcollide(plyr,‚¨áÔ∏è,0) then
			plyr.dy = 0
			
			-- reset jump count
			plyr.jumps = plyr.basejumps

			-- correct y position if
			-- fell into ground
			plyr.y -= plyr.y%8
		end -- /if mcollide down
	
	end -- /if plyr.dx </> 0

	-- test collision on left
	if plyr.dx < 0 then
	
		-- prevent movement through
		-- walls to the left
		if mcollide(plyr,‚¨ÖÔ∏è,0) then
			plyr.dx = 0
		
			-- correct x position to
			-- avoid getting stuck in wall
			plyr.x=ceil((plyr.x-1)/8)*8
		end -- /if mcollide left
	
	-- test collision on right
	elseif plyr.dx > 0 then
	
		-- prevent movement through
		-- walls to the right
		if mcollide(plyr,‚û°Ô∏è,0) then
			plyr.dx = 0
		
			-- correct x position to
			-- avoid getting stuck in wall
			plyr.x=flr((plyr.x+1)/8)*8
			--plyr.x -= plyr.x%8
		end -- /if mcollide right
	
	end -- /if plyr.dx </> 0
	
	-- update x,y by the calculated
	-- change (delta x, delta y)
	plyr.x += plyr.dx
	plyr.y += plyr.dy
	
end -- /function move_plyr()
