-- *** level warp function ***
-- (call in _update or plyr_move)
function level_warp()

	-- cheat code: press z to
	-- skip to end of level
	-- (to save time when testing)
	if btnp(üÖæÔ∏è) then
	
		-- move x position to the
		-- right end of the map
		plyr.x = 126*8
		
		-- depending on the level,
		-- move the y position
		-- accordingly
		if level == 1 then
			plyr.y = 11*8
		elseif level == 2 then
			plyr.y = 27*8
		elseif level == 3 then
			plyr.y = 43*8
		elseif level == 4 then
			plyr.y = 60*8
		end -- /if/elseif level
	end -- /if btnp(üÖæÔ∏è)

	-- warp to next level after
	-- crossing right edge of map
	if plyr.x >= 1024-plyr.w/2 then
		
		-- victory chime
		sfx(1)
		
		-- update level number
		level = level + 1
		
		-- move the player to the
		-- start of next level and
		-- reset the camera
		
		-- move x position to the
		-- left end of the map
		plyr.x = 2*8
		
		-- update the player y and
		-- camera y accordingly
		if level == 2 then
			plyr.y = 27*8
			camy = 16*8 
		elseif level == 3 then
			plyr.y = 43*8
			camy = 32*8
		elseif level == 4 then
			plyr.y = 60*8
			camy = 48*8
		else
			-- return to start after
			-- last level
			plyr.y = 11*8
			camy = 0 
			level= 1
		end -- /if/elseif level
		
		-- update the respawn coords
		plyr.startx = plyr.x
		plyr.starty = plyr.y
		
	end -- /if plyr.x > 1024
	
end -- /function checkpoint()
-->8
-- respawn function
-- (call in _update or plyr_move)
function respawn()

	-- flag number for pit tiles
	pit = 7
	
	-- check for collision
	-- with pit tile
	if mcollide(plyr,‚¨áÔ∏è,pit)
	and plyr.dy > 0 then

		-- lose a life
		plyr.lives -= 1 

		-- play sound
		sfx(0)

		-- return to start
		plyr.x = plyr.startx
		plyr.y = plyr.starty 
		
	end -- /if mcollide down 
	
end -- /function respawn()
