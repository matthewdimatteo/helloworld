-- swap camera mode
function swap_cam()

	-- press x to swap cam mode
	if btnp(‚ùé) then
	
		if mode == "follow" then
			mode = "screen"
		else
			mode = "follow"
		end -- end if mode
	
	end -- end if btnp(üÖæÔ∏è)

end -- end function swap_cam()
