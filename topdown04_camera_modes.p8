-- warp between corners to test
function warp()

	-- get sprite number of tile
	-- at player's position
	local n=mget(plyr.x,plyr.y)
	
	-- check for flag 6 or 7
	-- (reserved for warp tiles)
	warp6=fget(n,6)
	warp7=fget(n,7)
	
	-- warp to bottom right
	if warp6 then
		plyr.x=118
		plyr.y=58
	end -- end if warp6
	
	-- warp to top left
	if warp7 then
		plyr.x=10
		plyr.y=7
	end -- end if warp7
	
end -- end function warp()
