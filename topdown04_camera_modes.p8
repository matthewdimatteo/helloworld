-- camera follow
function cam_follow()

	-- center on player
	camx=8*plyr.x-64+plyr.w/2
	camy=8*plyr.y-64+plyr.h/2

	-- adjust at left edge
	if plyr.x < 8 then
		camx += 8*(7-plyr.x)+plyr.w/2
	end

	-- adjust at top edge
	if plyr.y < 8 then
		camy += 8*(7-plyr.y)+plyr.h/2
	end

	-- adjust at right edge
	if plyr.x > 127-8 then
		camx -= 8*(plyr.x-(127-7))
			+plyr.w/2
	end

	-- adjust at bottom edge
	if plyr.y > 63-8 then
		camy -= 8*(plyr.y-(63-7))
			+plyr.h/2
	end

	-- position the camera
 camera(camx,camy)
 
end -- end function cam_follow()
