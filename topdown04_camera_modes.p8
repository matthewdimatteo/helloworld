-- player
function make_plyr()
	plyr = {} -- player object
	plyr.n=64
	
	-- x,y tile coords, not pixels
	plyr.x=8
	plyr.y=8
	
	-- sprite width px)
	plyr.w=8
	plyr.h=8
	
	plyr.flip=false
	plyr.facing="down"
end -- end make_plyr()

function move_plyr()

	-- left
	if btnp(‚¨ÖÔ∏è) then
		tx = plyr.x - 1
		ty = plyr.y
		plyr.dir = "left"
		plyr.n = 66
		plyr.flip = true
	end -- end if btnp(‚¨ÖÔ∏è)
	
	-- right
	if btnp(‚û°Ô∏è) then
		tx = plyr.x + 1
		ty = plyr.y
		plyr.dir = "right"
		plyr.n = 66
		plyr.flip = false
	end -- end if btnp(‚û°Ô∏è)
	
	-- up
	if btnp(‚¨ÜÔ∏è) then
		ty = plyr.y - 1
		tx = plyr.x
		plyr.dir = "up"
		plyr.n = 65
		plyr.flip = false
	end -- end if btnp(‚¨ÜÔ∏è)
	
	-- down
	if btnp(‚¨áÔ∏è) then
		ty = plyr.y + 1
		tx = plyr.x
		plyr.dir = "down"
		plyr.n = 64
		plyr.flip = false
	end -- end if btnp(‚¨áÔ∏è)
	
	-- sprite number of target tile
	tn = mget(tx,ty)
 
	-- true/false is flag 0 on
	-- for that sprite
	is_solid = fget(tn,solid)
 
	-- move to target if not solid
	if is_solid == false then
		plyr.x=tx
		plyr.y=ty
	end -- end if is_solid == false
	
	-- constrain player to map
	if plyr.x < 0 then
		plyr.x = 0
	end
	if plyr.y < 0 then
		plyr.y = 0
	end
	if plyr.x > 127 then
		plyr.x = 127
	end 
	if plyr.y > 63 then
		plyr.y = 63
	end
	
end -- end move_plyr()
