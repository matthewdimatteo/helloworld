-- camera
function cam_follow()
	camx = plyr.x - 64 + plyr.w/2
	--camy = 0
	
	-- pin camera to left edge
	if camx < 0 then
		camx = 0
	end
	
	-- pin camera to right edge
	if plyr.x >= 1024-64-plyr.w/2
	then
		camx = 1024-128 
	end
	
	camera(camx,camy)
end -- end function cam_follow()
-->8
-- pits and respawning
function respawn()

	-- when falling into a pit,
	-- lose a life and respawn the
	-- player at the start
	if mcollide(plyr,"down",7)
	and plyr.dy > 0 then
		lives = lives - 1 
		sfx(2) -- play failure sound
		plyr.x = plyr_start_x
		plyr.y = plyr_start_y 
	end -- end if mcollide down
	
end -- end function respawn()
-->8
-- animate player sprite
function animate()

	-- jumping sprite
	if plyr.landed == false then
		plyr.n = 6
	
	-- run cycle when moving l/r
	elseif btn(⬅️) or btn(➡️) then
	
		-- how often the sprite
		-- should change
		animrate = 0.16
		
		-- change sprite after an
		-- interval of time
		if time()-animtime > animrate
		then
				
			-- reset timer
			animtime=time()
				
			-- go to next sprite
			plyr.n = plyr.n + 1
			
			-- loop back to first frame
			if plyr.n > 3 then
				plyr.n = 1
			end -- end if plyr.n
			
		end -- end if time
		
	-- idle/default sprite
	else
		plyr.n = 1
	end -- end if/else landed

end -- end function animate()
-->8
-- warp to next level
function warp()

	-- cheat code: press z to
	-- skip to end of level
	if btnp(🅾️) then
		if level == 1 then
			plyr.x = 124*8
			plyr.y = 11*8
		elseif level == 2 then
			plyr.x = 123*8
			plyr.y = 27*8
		elseif level == 3 then
			plyr.x = 121*8
			plyr.y = 43*8
		end -- end if level 1/2/3
	end -- end if btnp(🅾️)

	-- warp to level 2
	if plyr.x >= 1024-plyr.w/2 then
		
		level = level + 1
		
		-- move the player to the
		-- start of next level and
		-- reset the camera
		if level == 2 then
			plyr.x = 4*8
			plyr.y = 27*8
			camy = 16*8 
		elseif level == 3 then
			plyr.x = 4*8
			plyr.y = 43*8
			camy = 32*8
		else
			-- return to start after
			-- last level
			plyr.x = 4*8
			plyr.y = 11*8
			camy = 0 
			level = 1
		end -- end if level == 2/3
		
		-- update the respawn coords
		plyr_start_x = plyr.x
		plyr_start_y = plyr.y
		
	end -- end if plyr.x > 1024
	
end -- end function warp()
